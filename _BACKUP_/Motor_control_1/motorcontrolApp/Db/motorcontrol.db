record(ao, "$(P)move"){
    field(OUT, "$(MOTOR).VAL PP")
}

record(calcout, "$(P)mmtostep"){
    field(DESC, "converts mm to steps for the motor")
    field(INPA, "$(P)mminput")
    field(CALC, "A*200")
    field(OUT, "$(P)move")
    field(FLNK, "$(P)move")
}

record(ai, "$(P)mminput"){
    field(DESC, "mm to move the motor")
    field(EGU, "mm")
    field(FLNK, "$(P)mmtostep")
}
